{
  "name": "comdash",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start-server": "json-server --watch -p 8000 server.js --routes routes.json",
    "build-src": "react-scripts build && mkdir ./build/fixtures && cp ./fixtures/* ./build/fixtures/ && cp ./web.config ./build/",
    "make-zip": "zip -er build.zip ./build/",
    "build": "yarn build-src && yarn make-zip",
    "test": "yarn lint && yarn typecheck && yarn test-src",
    "eject": "react-scripts eject",
    "flow": "flow",
    "test-src": "node node_modules/jest/bin/jest.js",
    "test-travis": "yarn lint && yarn test-src",
    "typecheck": "flow",
    "lint": "eslint --max-warnings 0 ."
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "class-autobind": "^0.1.4",
    "d3": "^3.5.17",
    "d3-shape": "^1.2.0",
    "detect-browser": "^1.9.0",
    "dom-to-image": "^2.5.2",
    "file-saver": "^1.3.3",
    "formik": "^0.8.9",
    "json2csv": "^3.11.2",
    "jspdf": "^1.3.5",
    "jszip": "^3.1.4",
    "kf-react-bubble-chart": "^7.4.1",
    "lodash": "^4.17.4",
    "material-ui": "^0.18.6",
    "pdfjs-dist": "^1.9.575",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-burger-menu": "^2.1.4",
    "react-dnd": "^2.5.1",
    "react-dnd-html5-backend": "^2.5.1",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.2",
    "react-primitives": "^0.4.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-swipeable-views": "^0.12.8",
    "react-tap-event-plugin": "^2.0.1",
    "react-transition-group": "^2.0.2",
    "react-waypoint": "^7.0.4",
    "recharts": "^1.0.0-alpha.2",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.4",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.19",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.49.1",
    "json-server": "^0.11.2",
    "react-addons-test-utils": "^15.6.0",
    "react-scripts": "1.0.10",
    "react-test-renderer": "^15.6.1",
    "shx": "^0.2.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": ["./src/jestInit.js"],
    "unmockedModulePathPatterns": [
      "/node_modules/debug",
      "/node_modules/react",
      "/node_modules/material-ui",
      "/node_modules"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
